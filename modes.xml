<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <!-- hin â†’ pan -->

  <mode name="hin-pan_Arab" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="hin-pan_Arab.automorf.bin"/>
      </program>


      <program name="cg-proc -w">
        <file name="hin-pan_Arab.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="hin-pan_Arab.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="hin-pan_Arab.autobil.bin"/>
      </program>
      <program name="lrx-proc -m">
        <file name="hin-pan_Arab.autolex.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-hin-pan.hin-pan.t1x"/>
        <file name="hin-pan.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-hin-pan.hin-pan.t2x"/>
        <file name="hin-pan.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-hin-pan.hin-pan.t3x"/>
        <file name="hin-pan.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="hin-pan_Arab.autogen.bin"/>
      </program>

    </pipeline>
  </mode>

  <mode name="hin-pan_Guru" install="yes">
